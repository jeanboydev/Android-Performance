<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeanboy.app.performance">

    <application
        android:name=".base.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Performance">
        <activity android:name=".ui.activity.IdleHandlerActivity"></activity>
        <activity android:name=".ui.activity.Handler3Activity" />
        <activity android:name=".ui.activity.Handler2Activity" />
        <activity android:name=".ui.activity.Handler1Activity" />
        <activity android:name=".ui.activity.HandlerListActivity" />
        <activity android:name=".memory.LeakActivity" />
        <activity android:name=".memory.MemoryActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>